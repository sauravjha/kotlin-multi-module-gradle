pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "This is building stage"
                sh './gradlew build'
            }
        }
        stage('Test') {
            steps {
                echo "This is testing stage"
                 sh './gradlew test'
            }
        }
        stage('Deploy') {
            steps {
                echo "This is deploy Stage"
            }
        }
    }
}